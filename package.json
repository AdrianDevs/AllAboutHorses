{
  "name": "horse-compare",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3016",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
